<div class="signup_form" id="center_forms">
<h1> Edit your profile</h1>

<%= form_for @user do |f| %>
  <%= f.label "Email" %>
  <%= f.text_field :user_email %>

  <%= f.label "First Name" %>
  <%= f.text_field :user_name_first %>

  <%= f.label "Last Name" %>
  <%= f.text_field :user_name_last %>

  <%= f.label "Zip Code" %>
  <%= f.text_field :user_zip %>

  <%= f.label "Password" %>
  <%= f.password_field :password %>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
<div id="new_signup_button">
  <%= f.submit "Edit", class: 'signup_form_button'  %>
</div>

<% end %>
</div>
