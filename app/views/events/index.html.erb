<% if @events.any? %>
    <% @events.each do |event| %>
      <h1><%= event.name %></h1>
      <ul>
        <li>WHEN:<%= event.event_date %> at <%= event.event_time %></li>

      <li>WHERE:<%= event.event_address %>,<%= event.event_city %>, <%= event.event_state %>, <%= event.event_zip %></li>
      <li>AGE REQUIREMENT: <%= event.event_age_min %>+ </li>
      <p>DESCRIPTION: <%= event.event_description %></p>
    </ul>
    <% end %>
<% else %>
  <p>There are no events.</p>
<% end %>
<a href="/events/new">Create an Event</a>
